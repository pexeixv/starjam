{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "types": ["@astrojs/image/client"],
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@data/*": ["src/data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@js/*": ["src/js/*"],
      "@css/*": ["src/css/*"],
      "@/*": ["src/*"]
    }
  }
}
